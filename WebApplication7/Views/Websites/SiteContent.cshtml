@model WebApplication7.Models.RSSFeed

@{
    ViewBag.Title = "SiteContent";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>SiteContent</h2>
@{
    if (Model != null)
    {
        for (int i = 0; i < Model.feeditems.Count(); ++i)
        {
                <tr>
                    <td style="font-family:Verdana; font-size:12px;">
                        <p>@Model.feeditems[i].title</p>
                    </td>
                    <td style="font-family:Verdana; font-size:12px;">
                        <p><a href="http://@Html.Raw(Model.feeditems[i].link)">@Model.feeditems[i].link</a></p>
                    </td>
                    <td style="font-family:Verdana; font-size:12px;">
                        <p>@Html.Raw(Model.feeditems[i].description)</p>
                    </td>

                </tr>
            }
    }
}